public class Main {
    public static void main(String[] args) {
        // Unicode –¥–ª—è –≤—ã–±—Ä–∞–Ω–Ω—ã—Ö —Å–º–∞–π–ª–∏–∫–æ–≤
        String emoji1 = "\uD83D\uDE00";  // üòÄ
        String emoji2 = "\uD83D\uDE02";  // üòÇ
        String emoji3 = "\uD83D\uDE0D";  // üòç

        // –í—ã–≤–æ–¥–∏–º —Å–º–∞–π–ª–∏–∫–∏ –Ω–∞ —ç–∫—Ä–∞–Ω
        System.out.println(emoji1);
        System.out.println(emoji2);
        System.out.println(emoji3);
    }
}
