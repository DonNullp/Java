public class Main {
    public static void main(String[] args) {
        // Вихідні дані
        int i = 2; // Наприклад, i = 2
        float f = 2.5f; // Наприклад, f = 2.5
        byte b = 3; // Наприклад, b = 3

        // Обчислюємо вираз
        double y = (Math.pow(i, 5) / Math.log(f)) - (Math.sin(f) / b);

        // Виводимо результат
        System.out.println("Значення y: " + y);
    }
}
