public class Main {
    public static void main(String[] args) {
        String s1 = Integer.toString(150); // Перетворення цілого числа в рядок
        String s2 = Character.valueOf('5').toString(); // Перетворення символу в рядок

        // Вивід результату на екран
        System.out.println(s1 + s2); // Об'єднання рядків
    }
}
